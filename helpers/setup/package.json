{
  "name": "meiosis-setup",
  "version": "5.0.0",
  "private": true,
  "description": "Meiosis Helpers - setup",
  "main": "meiosis-setup.js",
  "types": "index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "watch": "npm run check -- --watch",
    "lib": "tsc",
    "build": "rollup -c && cp -r src/* dist/",
    "start": "npm run lib && npm run build && cp README.md dist/ && cat package.json|grep -v private>dist/package.json",
    "test": "jest --config jestconfig.json",
    "docs": "typedoc --excludeNotExported --out docs/ src",
    "onchange": "onchange -i index.ts 'state/*' 'router-helper/*' -- npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foxdonut/meiosis.git"
  },
  "keywords": [
    "meiosis",
    "reactive",
    "state",
    "state management"
  ],
  "author": "foxdonut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foxdonut/meiosis/issues"
  },
  "homepage": "https://github.com/foxdonut/meiosis#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "flyd": "^0.2.8",
    "immer": "^6.0.3",
    "jest": "^25.4.0",
    "mergerino": "0.4.0",
    "mithril": "^2.0.4",
    "onchange": "^6.1.1",
    "prettier": "2.0.5",
    "ramda": "^0.27.0",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "^25.4.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  }
}
